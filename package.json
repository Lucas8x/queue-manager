{
  "name": "queue-manager",
  "private": true,
  "author": "Lucas Emanuel <lucasemanuel8x@gmail.com>",
  "scripts": {
    "prebuild": "bun run --filter '@queue/lib' build",
    "build": "bun run --filter '!@queue/lib' build",
    "dev:back": "bun run ./packages/backend/src/index.ts",
    "dev:back:seed": "bun run ./packages/backend/src/index.ts --seed",
    "dev:front": "bun run --filter '@queue/dashboard' dev",
    "build:front": "bun run --filter '@queue/dashboard' build",
    "build:back": "bun run --filter '@queue/backend' build"
  },
  "workspaces": ["packages/*"],
  "homepage": "https://github.com/Lucas8x/queue-manager",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Lucas8x/queue-manager.git"
  },
  "bugs": {
    "url": "https://github.com/Lucas8x/queue-manager/issues"
  },
  "catalog": {
    "dayjs": "^1.11.19",
    "elysia": "latest",
    "@types/bun": "latest",
    "@types/node": "^24.7.2"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.6"
  },
  "peerDependencies": {
    "typescript": "^5.8.3"
  },
  "packageManager": "bun@1.2.19",
  "engines": {
    "bun": ">=1.2.19"
  },
  "keywords": [
    "queue",
    "task",
    "manager",
    "dashboard",
    "job",
    "lib",
    "typescript",
    "bun"
  ]
}
